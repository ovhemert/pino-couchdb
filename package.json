{
  "name": "pino-couchdb",
  "version": "1.0.0",
  "description": "A transport for pino that sends messages to CouchDB",
  "homepage": "https://github.com/ovhemert/pino-couchdb",
  "main": "src/index.js",
  "scripts": {
    "dev:cli": "node ./test/debug.js | node ./src/cli.js",
    "dev:couchdb": "docker run -it -p 5984:5984 couchdb",
    "lint": "standard",
    "test": "standard && tap test/*.test.js --coverage --100",
    "test:appveyor": "standard && tap test/*.test.js --coverage",
    "test:report": "standard && tap test/*.test.js --coverage  --coverage-report=html --100",
    "posttest": "tap --coverage --coverage-report=lcovonly"
  },
  "bin": {
    "pino-couchdb": "src/cli.js"
  },
  "keywords": [
    "pino",
    "pino-transport",
    "couch",
    "couchdb",
    "logging"
  ],
  "author": "Osmond van Hemert <osmond@vanhemert.nu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ovhemert/pino-couchdb.git"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "precommit": [
    "lint",
    "test"
  ],
  "devDependencies": {
    "pino-multi-stream": "^4.0.2",
    "sinon": "^7.2.5",
    "standard": "^12.0.0",
    "tap": "^13.0.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "batch2": "^1.0.1",
    "commander": "^2.19.0",
    "fast-json-parse": "^1.0.3",
    "pumpify": "^1.5.1",
    "split2": "^3.1.1",
    "stream": "0.0.2"
  }
}
